{
	"extends": "@greybox/shared-tsconfig/tsconfig.json",
	"include": ["cloudflare-worker-config/types.d.ts", "**/*.ts", "**/*.tsx"],
	"compilerOptions": {
		"types": [
			"@remix-run/cloudflare",
			"@cloudflare/workers-types",
			"vite/client",
			"cloudflare-worker-config/types.d.ts"
		],
		"jsx": "react-jsx",

		"baseUrl": ".",
