{
	"$schema": "https://turbo.build/schema.json",
	"extends": ["//"],
	"tasks": {
		"deploy": {
			"dependsOn": ["web-app#build"],
			"inputs": ["wrangler.json", "../../web-app/build/**/*"],
			"outputLogs": "new-only",
			"passThroughEnv": ["CLOUDFLARE_API_TOKEN", "CLOUDFLARE_ACCOUNT_ID"]
		},
		"start": {
			"dependsOn": ["web-app#build"]
		},
		"dev:check": {
			"cache": false,
			"dependsOn": ["dev:check-worker"]
		},
		"dev:check-worker": {
			"cache": false
		}
	}
}
