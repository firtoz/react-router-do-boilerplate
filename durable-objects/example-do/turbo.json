{
	"$schema": "https://turbo.build/schema.json",
	"extends": ["//"],
	"tasks": {
		"dev": {
			"dependsOn": ["worker-app#dev:check-worker"]
		},
		"dev:check": {
			"cache": false
		},
		"deploy-durable-objects": {
			"inputs": ["wrangler.json", "src/**/*"],
			"outputLogs": "new-only",
			"passThroughEnv": ["CLOUDFLARE_API_TOKEN", "CLOUDFLARE_ACCOUNT_ID"]
		}
	}
}
